<!DOCTYPE html>
<html leng="en" dir="ltr">
    <head>
        <meta charset="utf-8">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/style.css">
        <title>customer</title>
     <link rel="stylesheet" href="css/style.css">
    </head>
    <body>
      
      <form action="/click" method="post">
        <div class="box" id="heading">
          <h1>Available Vehicle List</h1>
        </div>
        <div>
          <h4><%=content%></h4>
        </div>
      <input type="string" id="fname" name="user" value="<%=user%>">
      <div class="box">
        <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Model Name</th>
            <th scope="col">Car Year</th>
            <th scope="col">Car Number</th>
            <th scope="col">Car Transmission</th>
            <th scope="col">Price</th>
          </tr>
        </thead>
      
        <% for(var i=0;i<newListItems.length;i++){ %>
          <div class="item">
                  <tbody>
                    <tr>
                      <th scope="row"><%=i+1%></th>
                      <td><%=newListItems[i].modelName%></td>
                      <td><%=newListItems[i].carYear%></td>
                      <td><%=newListItems[i].carNo%></td>
                      <td><%=newListItems[i].carTrans%></td>
                      <td><%=newListItems[i].carRate%></td>
                      <td> 
                       
                        <button type="submit" name="click" class="dis" value="<%=newListItems[i]._id%>">click</button>


                      <td> <form action="/clicks" method="post">
                        <button type="submit" name="click" class="dis" value="<%=newListItems[i]._id%>">click</button>

                       </form></td>

                    </tr>
               
                 
        </div>
   <%} %>
  </table>
  <h1>Review</h1>
<form class="text-start" action="/review" method="post">
    <div class="form-group">
<div class="mb-3">
    <label for="" class="">Enter your review</label> 
    <textarea name="reviewBody" class="form-control" cols="5" rows="4"></textarea>
</div>
</div>
<button type="submit" class="btn btn-primary" name="review" value="<%=user%>">Publish</button>
</form>
  <form action="/logout" method="POST">
    <button type="submit" name="signout" class="signout">Sign Out</button> 
  </form>        
    </div>
    <script src="app.js"></script>
    </body>
</html>
